int pin=8; int pin1=2; int pin2=3; int pin3=4; int pin4=5; int value; void setup() { Serial.begin(9600); pinMode(pin,INPUT);

pinMode(pin1,OUTPUT); pinMode(pin2,OUTPUT); pinMode(pin3,OUTPUT); pinMode(pin4,OUTPUT);

}

void loop() { value=digitalRead(pin);

if (digitalRead(pin)==0) { digitalWrite(pin1,HIGH); digitalWrite(pin2,HIGH); } else digitalWrite(pin1,LOW); digitalWrite(pin2,LOW); }
